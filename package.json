    }
  },
  "engines": {
    "node": ">=14.x"
  },
  "scripts": {
    "postinstall": "npm run build",
    "build": "tsup",
    "rebuild": "npm run build",
    "lint": "eslint **/*.js",
    "lint:fix": "npm run lint -- --fix"
  },
  "devDependencies": {
    "@types/node": "20.6.2",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-prettier": "5.2.1",
