{
  "name": "rapid-electron-ipc",
  "version": "1.0.0",
  "author": "xuanguyen<xuan.0211@gmail.com>",
  "license": "MIT",
  "description": "Better ipc communication for electronï¼Œbased on vscode source code.",
  "keywords": [
    "electron",
    "vscode",
    "browser",
    "tab",
    "ipc"
  ],
  "homepage": "https://github.com/xuanhoa88/rapid-electron-ipc",
  "exports": {
    "./main": {
      "types": "./dist/main/index.d.ts",
      "import": "./dist/main/index.mjs",
